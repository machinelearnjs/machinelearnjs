# {{ name }}

{{{ comment.shortText }}}
{{{ comment.text }}}

the kind {{ this.kind }}
{{#ifEquals this this.kind 64}}
    {{#isSignatureValid this}}
        {{#each this.signatures}}
            {{newLine}}▸ **{{this.name}}**{{methodBracket this.parameters}}
            {{newLine}}{{{this.comment.shortText}}}
            {{newLine}}{{{this.comment.text}}}
            {{#filterTagExample this.comment.blockTags}}

                {{newLine}}### Usage
                {{#each this}}
                    {{#each this.content}}
                      {{newLine}}```{{{this.text}}}```
                    {{/each}}
                {{/each}}
            {{/filterTagExample}}
        {{/each}}
        {{newLine}}Defined in {{getSourceLink this.sources}}

        {{#each this.signatures}}
            {{#if this.parameters}}
                {{newLine}}**Parameters:**
                {{newLine}}{{constructParamTable this.parameters}}
            {{/if}}
            {{#if this.type}}
                {{newLine}}**Returns:**
                {{newLine}}{{renderMethodReturnType this.type}}
            {{/if}}
        {{/each}}
    {{/isSignatureValid}}
{{else}}
    {{#filterTagExample this.comment.blockTags}}
        {{newLine}}### Usage
        {{#each this}}
            {{#each this.content}}
              {{newLine}}```{{{this.text}}}```
            {{/each}}
        {{/each}}
    {{/filterTagExample}}

    {{#filterConstructor children}}
        {{newLine}}### Constructors
        {{#each this}}
            {{newLine}}* [constructor](#constructor)
        {{/each}}
    {{/filterConstructor}}

    {{#filterProperty children}}
        {{newLine}}### Properties
        {{#each this}}
            {{newLine}}* [{{this.name}}](#▸-{{cleanHyperLink this.name}})
        {{/each}}
    {{/filterProperty}}

    {{#filterMethod children}}
        {{newLine}}### Methods
        {{#each this}}
            {{newLine}}* [{{this.name}}](#λ-{{cleanHyperLink this.name}})
        {{/each}}
    {{/filterMethod}}



    {{#filterConstructor children}}

        {{newLine}}# Constructors
        {{newLine}}---
        {{#each this}}
            {{newLine}}### {{this.name}}
            {{newLine}}{{ this.comment.shortText }}
            {{newLine}}{{ this.comment.text }}

            {{#isSignatureValid this}}
                {{#each this.signatures}}
                    {{newLine}}⊕ **{{this.name}}**{{methodBracket this.parameters}}
                {{/each}}
                {{newLine}}Defined in {{getSourceLink this.sources}}
                {{newLine}}{{this.comment.shortText}}

                    {{#each this.signatures}}
                        {{newLine}}**Parameters:**
                        {{newLine}}{{constructParamTable this.parameters}}
                        {{newLine}}
                        {{newLine}}**Returns:** {{ this.type.name }}
                    {{/each}}
            {{else}}
                {{newLine}}No parameters required to invoke!
            {{/isSignatureValid}}

        {{/each}}
    {{/filterConstructor}}



    {{#filterProperty children}}

        {{newLine}}# Properties
        {{newLine}}---
        {{#each this}}
            {{newLine}}### ▸ {{this.name}}
            {{newLine}}{{ this.comment.shortText }}
            {{newLine}}{{ this.comment.text }}
            {{newLine}}Defined in {{getSourceLink this.sources}}
        {{/each}}
    {{/filterProperty}}

    {{#filterMethod children}}

        {{newLine}}# Methods
        {{newLine}}---
        {{#each this}}
            {{#isSignatureValid this}}
                {{#each this.signatures}}
                    {{newLine}}### λ {{this.name}}
                    {{newLine}}{{ this.comment.shortText }}
                    {{newLine}}{{ this.comment.text }}
                {{/each}}
                {{newLine}}Defined in {{getSourceLink this.sources}}
                {{newLine}}{{this.comment.shortText}}

                {{#each this.signatures}}
                    {{#if this.parameters}}
                        {{newLine}}**Parameters:**
                        {{newLine}}{{constructParamTable this.parameters}}
                    {{/if}}
                    {{#if this.type}}
                        {{newLine}}**Returns:**
                        {{newLine}}{{renderMethodReturnType this.type}}
                    {{/if}}
                {{/each}}
            {{else}}
                {{newLine}} Empty parameters required to invoke!
            {{/isSignatureValid}}
        {{/each}}
    {{/filterMethod}}

